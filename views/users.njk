{% extends 'layouts/layout.njk' %}
{% block content %}
  <h1 class="text-2xl font-bold">Users</h1>
  <ul class="space-y-4">
    {% for user in users %}
      <li class="flex items-center gap-4"><img src="{{ user.avatarUrl }}" class="w-10 h-10 rounded-full object-cover" alt="{{ user.name }}"/>{{ user.name }} - {{ user.username }} - {{ user.id }}</li>
    {% endfor %}
  </ul>
{% endblock %}
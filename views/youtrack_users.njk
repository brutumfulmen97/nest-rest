{% extends 'layouts/layout.njk' %}
{% block content %}
  <h1 class="text-2xl font-bold">{{ title }}</h1>
  {% for user in users %}
    <div class="flex items-center gap-2">
      <img src="{{ user.avatar.url }}" alt="{{ user.login }}" class="w-10 h-10 rounded-full object-cover"/>
      <p>{{ user.login }}</p>
    </div>
  {% endfor %}
{% endblock %}